<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-smadp" version="0.0.1">
   <name>SMADP</name>
   <DESCRIPTION>SMADP Plugin</DESCRIPTION>
   <js-module name="SMADP" src="www/SMADP.js">
      <clobbers target="SMADP" />
   </js-module>
   <platform name="ios">
      <config-file parent="/*" target="config.xml">
         <feature name="SMADP">
            <param name="ios-package" value="SMADP" />
         </feature>
      </config-file>
      <source-file src="src/ios/SMADP.m" />
   </platform>
</plugin>